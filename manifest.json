{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.1",
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "cookies"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
